                    </tr>
                    <tr>
                        <th style="border: 1px solid black; text-align: left;">Peptide Sequences</th>
                        <td style="border: 1px solid black; text-align: right;">{{peptideSequencesCount}}</td>
                    </tr>
                </table>
            </p>
        {{/with}}
    {{/if}}
    {{#if lfqSummary}}
        {{#with lfqSummary}}
            <p>
                <h3 style="text-align: center;">Lfq Summary</h3>
                <table style="border: 1px solid black; border-collapse: collapse; max-width: 1000px; margin: auto;">
