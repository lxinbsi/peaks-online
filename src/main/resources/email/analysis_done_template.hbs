                    </tr>
                </thead>
                <tbody>
                    {{#each slSummary}}
                        <tr>
                            <td style="border: 1px solid black; text-align: left;">{{sampleName}}</td>
                            <td style="border: 1px solid black; text-align: right;">{{msRun}}</td>
                            <td style="border: 1px solid black; text-align: right;">{{ms1Count}}</td>
                            <td style="border: 1px solid black; text-align: right;">{{ms2Count}}</td>
                            <td style="border: 1px solid black; text-align: right;">{{psmCount}}</td>
                            <td style="border: 1px solid black; text-align: right;">{{peptideCount}}</td>
                            <td style="border: 1px solid black; text-align: right;">{{sequenceCount}}</td>
